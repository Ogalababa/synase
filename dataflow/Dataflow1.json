{
	"name": "Dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dwh_dim_location",
						"type": "DatasetReference"
					},
					"name": "locationID"
				},
				{
					"dataset": {
						"referenceName": "dwh_dim_employee_go",
						"type": "DatasetReference"
					},
					"name": "EmployeeID"
				},
				{
					"linkedService": {
						"referenceName": "lsconnection",
						"type": "LinkedServiceReference"
					},
					"name": "salary"
				},
				{
					"linkedService": {
						"referenceName": "lsconnection",
						"type": "LinkedServiceReference"
					},
					"name": "Bonus"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          LocationID as string,",
				"          Adress as string,",
				"          City as string,",
				"          Country as string,",
				"          State as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> locationID",
				"source(output(",
				"          Employeekey as string,",
				"          EployeeID as integer,",
				"          DepartmentID as integer,",
				"          Firstname as string,",
				"          Lastname as string,",
				"          Department as string,",
				"          Employeestatus as integer,",
				"          Startdate as date,",
				"          Enddate as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> EmployeeID",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet',",
				"     fileSystem: 'fsngroup6',",
				"     folderPath: 'ANC',",
				"     fileName: 'dbo.anc_employee.parquet') ~> salary",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet',",
				"     fileSystem: 'fsngroup6',",
				"     folderPath: 'ACHRM',",
				"     fileName: 'dbo.achrm_bonus.parquet') ~> Bonus",
				"EmployeeID derive(LocationID = ERROR_FUNCTION('')) ~> derivedColumn1"
			]
		}
	}
}